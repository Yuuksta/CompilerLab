int globle_i
char globle_c
float price
int main(int argc, char argv) {
    int a
    float b
    char c
    a = 2
    b = 3.0
    globle_i = 10
    while(!globle_i) {
        a = (a * b - 1) / 2 
    }
    price = a
    a++
    b++
    if(a > 10) {
        c = '!'
        a = a && b
    }
    return price
}
